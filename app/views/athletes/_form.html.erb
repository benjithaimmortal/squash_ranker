<%= form_for @athlete, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <br>

  <%= f.label :level %>
  <%= f.select :level, [1, 2, 3, 4, 5] %>

  <br>

  <%= f.label :positive %>
  <%= f.text_field :positive %>

  <br>

  <%= f.label :negative %>
  <%= f.text_field :negative %>

  <br>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
